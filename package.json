    "ember-template-lint": "^2.11.0",
    "ember-test-selectors": "^5.0.0",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.8.0",
    "eslint-plugin-ember": "^8.13.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "qunit-dom": "^1.4.0",
    "tailwindcss": "^1.8.10"
